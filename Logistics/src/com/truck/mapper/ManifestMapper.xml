<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.truck.dao.IManifestDao">
	
	<select id="selectManifestByUserRequirementId"  parameterType="int" resultMap="ManifestMap">
		select m_id, m_name, m_count, m_r_id from t_manifest where m_id =#{id}
	</select>
	
	<resultMap id="ManifestMap" type="ManifestBean">
		<id property="id"  column="t_id" ></id>
		<result property="name" column="m_name" />
		<result property="count" column="m_count" /> 
		<association property="userRequirementBean" column="m_r_id" select="com.truck.dao.IUserRequirementDao.selectByPrimaryKey" fetchType="lazy"/>
	</resultMap>
</mapper>